local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "In Another Time", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroEnabled = false})

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


local itemsspawner = Tab:AddSection({
	Name = "Item Giver"
})

local maptper = Window:MakeTab({
	Name = "Map Tp's",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Itemtp = Window:MakeTab({
	Name = "Item Tp's",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


itemsspawner:AddButton({
	Name = "Give Arrow",
	Callback = function()
    game:GetService("ReplicatedStorage").ItemGiver.GiveArrow:FireServer()
end    
})

local items = Itemtp:AddSection({
	Name = "Items"
})

local funmain = Tab:AddSection({
	Name = "Fun"
})

items:AddButton({
	Name = "Dio's Bone",
	Callback = function()
        local pl = game.Players.LocalPlayer.Character.HumanoidRootPart
        local humanoid = game.Players.LocalPlayer.Character.Humanoid
        game.Workspace:WaitForChild("Dio's Bone")
        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        wait(0.1)
        pl.CFrame = game.Workspace["Dio's Bone"].Handle.CFrame
end   
})

items:AddButton({
	Name = "Dio's Diary",
	Callback = function()
        local pl = game.Players.LocalPlayer.Character.HumanoidRootPart
        local humanoid = game.Players.LocalPlayer.Character.Humanoid
        game.Workspace:WaitForChild("Dio's Diary")
        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        wait(0.1)
        pl.CFrame = game.Workspace["Dio's Diary"].Handle.CFrame
end   
})

items:AddButton({
	Name = "Requiem Arrow",
	Callback = function()
        local pl = game.Players.LocalPlayer.Character.HumanoidRootPart
        local humanoid = game.Players.LocalPlayer.Character.Humanoid
        game.Workspace:FindFirstChild("Requiem Arrow")
        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        wait(0.1)
        pl.CFrame = game.Workspace["Requiem Arrow"].Handle.CFrame
end   
})
local npcs = maptper:AddSection({
	Name = "NPCS"
})

local mapplaces = maptper:AddSection({
	Name = "Map TP's"
})

local plrtp = maptper:AddSection({
	Name = "Tp 2 Player"
})

plrtp:AddTextbox({
	Name = "GoTo",
	Default = "",
	TextDisappear = true,
	Callback = function(plr)
		local function get_player_from_partial(s)
            s = s:lower()
            for _, player in ipairs(game.Players:GetPlayers()) do
                if s == player.Name:lower():sub(1, #s) then
                    return player
                end
            end
            return nil
        end
        local target = get_player_from_partial(plr)
        local pl = game.Players.LocalPlayer.Character.HumanoidRootPart
        local humanoid = game.Players.LocalPlayer.Character.Humanoid
        game.Workspace:WaitForChild(tostring(target))
        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        wait(0.1)
        pl.CFrame = game.Workspace:WaitForChild(tostring(target))["HumanoidRootPart"].CFrame
	end	  
})

