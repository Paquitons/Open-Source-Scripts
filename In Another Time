funmain:AddTextbox({
	Name = "Ragdoll",
	Default = "",
	TextDisappear = true,
	Callback = function(plrtoknock)
		local function get_player_from_partial(s)
            s = s:lower()
            for _, player in ipairs(game.Players:GetPlayers()) do
                if s == player.Name:lower():sub(1, #s) then
                    return player
                end
            end
            return nil
        end
        local target = get_player_from_partial(plrtoknock)
        game:GetService("ReplicatedStorage").Basic.Ragdoll:FireServer(workspace:FindFirstChild(tostring(target)).Humanoid)
	end	  
})

