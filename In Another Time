        game:GetService("ReplicatedStorage").Attacks.DamageBlunt:FireServer(workspace:FindFirstChild(tostring(target)).Humanoid,0.1,CFrame.new(-817, -3, 1613),0.1,BrickColor.new("New Yeller"),0.6,"rbxassetid://542443306",1)
        game:GetService("ReplicatedStorage").Attacks.DamageBlunt:FireServer(workspace:FindFirstChild(tostring(target)).Humanoid,0.1,CFrame.new(-817, -3, 1613),0.1,BrickColor.new("New Yeller"),0.6,"rbxassetid://542443306",1)
	end	  
})

local humanoidsettings = Tab:AddSection({
	Name = "WalkSpeed/JumpPower"
})

humanoidsettings:AddSlider({
	Name = "Walkspeed",
	Min = 16,
	Max = 5000,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 16,
	ValueName = "Walkspeed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").WalkSpeed = Value
	end    
})

humanoidsettings:AddSlider({
	Name = "Jump Power",
	Min = 50,
	Max = 1000,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 16,
	ValueName = "Jump Power",
	Callback = function(Value)
		game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").JumpPower = Value
	end    
})
