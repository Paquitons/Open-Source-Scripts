funmain:AddTextbox({
	Name = "Kill",
	Default = "",
	TextDisappear = true,
	Callback = function(plr)
		local function get_player_from_partial(s)
            s = s:lower()
            for _, player in ipairs(game.Players:GetPlayers()) do
                if s == player.Name:lower():sub(1, #s) then
                    return player
                end
            end
            return nil
        end
        local target = get_player_from_partial(plr)
        game:GetService("ReplicatedStorage").Attacks.DamageHamonOrVamp:FireServer(workspace:FindFirstChild(tostring(target)).Humanoid,9999,CFrame.new(-817, -3, 1613),9999,BrickColor.new("New Yeller"),9999,"rbxassetid://542443306",1)
        game:GetService("ReplicatedStorage").Attacks.DamageBlunt:FireServer(workspace:FindFirstChild(tostring(target)).Humanoid,9999,CFrame.new(-817, -3, 1613),9999,BrickColor.new("New Yeller"),9999,"rbxassetid://542443306",1)
	end	  
})

funmain:AddButton({
	Name = "Bypass TS",
	Callback = function()
        local tsvalue = game.Lighting.TS
            if tsvalue.Value == true then
		    tsvalue.Value = false
	end
end
})
funmain:AddButton({
	Name = "20 Sec TS (only works if you have The World)",
	Callback = function()
    game:GetService("ReplicatedStorage").SpecialMoves.Timestop:FireServer(21,"Jotaro")
  	end    
})

